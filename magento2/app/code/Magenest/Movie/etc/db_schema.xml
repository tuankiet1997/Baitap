<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="magenest_director" resource="default" engine="innodb" comment="magenest director Table">
        <column xsi:type="int" name="director_id" padding="10" unsigned="false" nullable="false" identity="true" comment="Director ID"/>
        <column xsi:type="text" name="name" comment="Name"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="director_id"/>
        </constraint>
    </table>
    <table name="magenest_movie" resource="default" engine="innodb" comment="magenest movie Table">
        <column xsi:type="int" name="movie_id" padding="10"  unsigned="false" nullable="false" identity="true" comment="Movie ID"/>
        <column xsi:type="text" name="name" comment="Name"/>
        <column xsi:type="text" name="description" comment="Description"/>
        <column xsi:type="int" name="rating" padding="10" unsigned="false" nullable="false" default="0" comment="Rating"/>
        <column xsi:type="int" name="director_id" padding="10" unsigned="false" nullable="false" identity="false" comment="Director ID"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="movie_id"/>
        </constraint>
        <constraint xsi:type="foreign" referenceId="MAG_DIR_DIR_ID_MAG_MOV_DIR_ID"
                    table="magenest_movie" column="director_id" referenceTable="magenest_director"
                    referenceColumn="director_id" onDelete="CASCADE"/>
    </table>

    <table name="magenest_price_chair" resource="default" engine="innodb" comment="magenest chair price Table">
        <column xsi:type="int" name="id" padding="10"  unsigned="false" nullable="false" identity="true" comment="ID"/>
        <column xsi:type="int" name="movie_id" padding="10"  unsigned="false" nullable="false" identity="false" comment="Movie ID"/>
        <column xsi:type="decimal" name="price_movie" scale="4" precision="20" unsigned="false" nullable="true"
                default="0" comment="Price Movie"/>
        <column xsi:type="int" name="chair_id" padding="10"  unsigned="false" nullable="false" identity="false" comment="Chair ID"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="id"/>
        </constraint>
         <constraint xsi:type="foreign" referenceId="CHAIR_ID"
                    table="magenest_price_chair" column="chair_id" referenceTable="magenest_chair_type"
                    referenceColumn="chair_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="MOV_ID"
                    table="magenest_price_chair" column="movie_id" referenceTable="magenest_movie"
                    referenceColumn="movie_id" onDelete="CASCADE"/>
    </table>

    <table name="magenest_chair_type" resource="default" engine="innodb" comment="magenest chair Table">
        <column xsi:type="int" name="chair_id" padding="10"  unsigned="false" nullable="false" identity="true" comment="Chair ID"/>
        <column xsi:type="text" name="name" comment="Name"/>
        <column xsi:type="smallint" name="status" unsigned="true" nullable="false"
                identity="false" default="0" comment="Status"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="chair_id"/>
        </constraint>
    </table>

    <table name="magenest_chair" resource="default" engine="innodb" comment="magenest chair Table">
        <column xsi:type="int" name="id" padding="10"  unsigned="false" nullable="false" identity="true" comment="ID"/>
        <column xsi:type="int" name="chair_id" padding="10"  unsigned="false" nullable="false" identity="false" comment="Chair ID"/>
        <column xsi:type="text" name="name" comment="Name"/>
        <column xsi:type="smallint" name="status" unsigned="true" nullable="false"
                identity="false" default="0" comment="Status"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="id"/>
        </constraint>
        <constraint xsi:type="foreign" referenceId="CHARI_ID"
                    table="magenest_chair" column="chair_id" referenceTable="magenest_chair_type"
                    referenceColumn="chair_id" onDelete="CASCADE"/>
    </table>


    <table name="magenest_data_checkout" resource="default" engine="innodb" comment="magenest checkout Table">
        <column xsi:type="int" name="id" padding="10"  unsigned="false" nullable="false" identity="true" comment="ID"/>
        <column xsi:type="text" name="name" comment="Name"/>
        <column xsi:type="text" name="showtime" comment="Showtime"/>
        <column xsi:type="text" name="room" comment="Room"/>
        <column xsi:type="decimal" name="price_movie" scale="4" precision="20" unsigned="false" nullable="true"
                default="0" comment="Price Movie"/>
        <column xsi:type="text" name="chair" comment="Chair"/>
        <column xsi:type="text" name="typechair" comment="TypeChair"/>
        <column xsi:type="int" name="customer_id" padding="10"  unsigned="false" nullable="false" identity="false" comment="Customer ID"/>
        <column xsi:type="smallint" name="status" unsigned="true" nullable="false"
                identity="false" default="0" comment="Status"/>
        <column xsi:type="timestamp" name="created_at" on_update="false" nullable="false" default="CURRENT_TIMESTAMP"
                comment="Created At"/>
        <column xsi:type="timestamp" name="updated_at" on_update="true" nullable="false" default="CURRENT_TIMESTAMP"
                comment="Updated At"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="id"/>
        </constraint>
    </table>

    <table name="magenest_duplicate_chair" resource="default" engine="innodb" comment="magenest movie Table">
        <column xsi:type="int" name="id" padding="10"  unsigned="false" nullable="false" identity="false" comment="ID"/>
        <column xsi:type="int" name="chair_id" padding="10"  unsigned="false" nullable="false" identity="false" comment="Chair ID"/>
        <constraint xsi:type="foreign" referenceId="CHARI_AND_ID"
                    table="magenest_duplicate_chair" column="id" referenceTable="magenest_chair"
                    referenceColumn="id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="ID_AND_ID"
                    table="magenest_duplicate_chair" column="chair_id" referenceTable="magenest_chair_type"
                    referenceColumn="chair_id" onDelete="CASCADE"/>
    </table>

    <table name="magenest_actor" resource="default" engine="innodb" comment="magenest actor Table">
        <column xsi:type="int" name="actor_id" padding="10" unsigned="false" nullable="false" identity="true" comment="Actor ID"/>
        <column xsi:type="text" name="name" comment="Name"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="actor_id"/>
        </constraint>
    </table>
    <table name="magenest_movie_actor" resource="default" engine="innodb" comment="magenest movie Table">
        <column xsi:type="int" name="movie_id" padding="10"  unsigned="false" nullable="false" identity="false" comment="Movie ID"/>
        <column xsi:type="int" name="actor_id" padding="10" unsigned="false" nullable="false" identity="false" comment="Actor ID"/>
        <constraint xsi:type="foreign" referenceId="MAG_MOV_MOV_ID_MAG_MOV_MOV_ID"
                    table="magenest_movie_actor" column="movie_id" referenceTable="magenest_movie"
                    referenceColumn="movie_id" onDelete="CASCADE"/>
        <constraint xsi:type="foreign" referenceId="MAG_ACT_ACT_ID_MAG_MOV_ACT_ID"
                    table="magenest_movie_actor" column="actor_id" referenceTable="magenest_actor"
                    referenceColumn="actor_id" onDelete="CASCADE"/>
    </table>

